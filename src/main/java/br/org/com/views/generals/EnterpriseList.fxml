<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="420.0" prefWidth="621.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.org.com.controllers.generals.EnterpriseController">
   <children>
      <TableView fx:id="tableEnterprise" layoutX="19.0" layoutY="65.0" prefHeight="279.0" prefWidth="572.0">
        <columns>
          <TableColumn fx:id="code" prefWidth="75.0" text="Code" />
          <TableColumn fx:id="name" prefWidth="200.0" text="Foundation" />
          <TableColumn fx:id="email" prefWidth="80.0" text="E-Mail" />
          <TableColumn fx:id="phone" prefWidth="80.0" text="Phone" />
        </columns>
      </TableView>
      <Button layoutX="35.0" layoutY="28.0" mnemonicParsing="false" onAction="#add" style="-fx-background-color: LIMEGREEN;" text="+" textFill="#fffdfd">
         <font>
            <Font name="Arial Bold" size="18.0" />
         </font>
      </Button>
      <Button fx:id="buttonEdit" layoutX="77.0" layoutY="28.0" mnemonicParsing="false" onAction="#edit" prefHeight="33.0" prefWidth="35.0" style="-fx-background-color: ORANGE;" text="*" textFill="#f8f1f1">
         <font>
            <Font name="Arial Bold" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
