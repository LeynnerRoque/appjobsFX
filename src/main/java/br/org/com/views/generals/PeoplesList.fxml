<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.org.com.controllers.generals.PeopleController">


    <children>
        <TableView fx:id="tablePeoples" layoutX="19.0" layoutY="65.0" prefHeight="279.0" prefWidth="572.0">
            <columns>
                <TableColumn fx:id="code" prefWidth="75.0" text="Code" />
                <TableColumn fx:id="name" prefWidth="200.0" text="Foundation" />
                <TableColumn fx:id="email" prefWidth="80.0" text="E-Mail" />
                <TableColumn fx:id="phone" prefWidth="80.0" text="Phone" />
            </columns>
        </TableView>
      <Button layoutX="19.0" layoutY="32.0" mnemonicParsing="false" onAction="#addNew" style="-fx-background-color: LIMEGREEN;" text="+" textFill="#eeecec" />
      <Button layoutX="54.0" layoutY="31.0" mnemonicParsing="false" onAction="#edit" prefHeight="17.0" prefWidth="27.0" style="-fx-background-color: ORANGE;" text="*" textAlignment="CENTER" textFill="#f8f5f5">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Button>
    </children>


</AnchorPane>
