<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
    <persistence-unit name="appjobPU" transaction-type="RESOURCE_LOCAL">
        <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
        <class>br.org.com.model.Databasechangeloglock</class>
        <class>br.org.com.model.Enterprise</class>
        <class>br.org.com.model.Location</class>
        <class>br.org.com.model.JhiUser</class>
        <class>br.org.com.model.JhiPersistentAuditEvent</class>
        <class>br.org.com.model.JhiPersistentToken</class>
        <class>br.org.com.model.JhiAuthority</class>
        <class>br.org.com.model.JhiPersistentAuditEvtData</class>
        <class>br.org.com.model.Job</class>
        <class>br.org.com.model.Peoples</class>
        <properties>
            <property name="javax.persistence.jdbc.url" value="jdbc:mysql://localhost:3306/appjobs"/>
            <property name="javax.persistence.jdbc.user" value="root"/>
            <property name="javax.persistence.jdbc.driver" value="com.mysql.jdbc.Driver"/>
            <property name="javax.persistence.jdbc.password" value="roque*89"/>
        </properties>
    </persistence-unit>
</persistence>
